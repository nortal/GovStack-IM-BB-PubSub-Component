{
  "name": "imbb-management-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite -l info",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "test:unit": "vitest run",
    "test:unit-with-html-report": "vitest --reporter=default --reporter=html run",
    "test:unit-with-junit-report": "vitest --reporter=default --reporter=junit --outputFile.junit=vitest.xml run",
    "test:unit-with-reports": "vitest --reporter=junit --outputFile.junit=build/junit-report.xml --coverage --reporter=vitest-sonar-reporter --outputFile.vitest-sonar-reporter=build/sonar-report.xml run",
    "test:unit-ui": "vitest --ui",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "@fontsource/open-sans": "^5.0.9",
    "@mdi/font": "^7.2.96",
    "@vee-validate/i18n": "^4.11.3",
    "@vee-validate/rules": "^4.11.3",
    "@vuepic/vue-datepicker": "^6.1.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "deepmerge": "^4.3.1",
    "esbuild": "^0.18.16",
    "oidc-client-ts": "^2.2.5",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.0.11",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.17"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@tsconfig/node18": "^18.2.1",
    "@types/jest": "^29.5.4",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^18.17.11",
    "@types/webpack-env": "^1.18.1",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.34.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuetify": "^2.0.5",
    "jsdom": "^22.1.0",
    "lcov": "^1.16.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.34.3",
    "vitest-sonar-reporter": "^0.4.1",
    "vue-tsc": "^1.8.8"
  }
}
